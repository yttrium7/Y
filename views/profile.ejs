        <header>
            <h3> The subjects you enrolled</h3>
        </header>
        <div class="row">
        <div id="your-subject-1" class="4u 12u$(medium) box">
            <h4>Class name 1</h4>
            <ul class="alt">
                <li>Lesson 1 : description
                    <ul class="actions">
                        <li><a href="#" class="button small">Continue</a></li>
                    </ul>
                </li>
                <li>Lesson 2 : description
                    <ul class="actions">
                        <li><a href="#" class="button small">Start</a></li>
                    </ul>
                </li>
                <li>Lesson 3 : description</li>
                <li>Lesson 4 : description</li>
                <li>Lesson 5 : description</li>
            </ul>
        </div>
        <div id="your-subject-2" class="4u 12u$(medium) box">
            <h4>Class name 2</h4>
            <ul class="alt">
                <li>Lesson 1 : description
                    <ul class="actions">
                        <li><a href="#" class="button small">Continue</a></li>
                    </ul>
                </li>
                <li>Lesson 2 : description
                    <ul class="actions">
                        <li><a href="#" class="button small">Start</a></li>
                    </ul>
                </li>
                <li>Lesson 3 : description</li>
                <li>Lesson 4 : description</li>
                <li>Lesson 5 : description</li>
            </ul>
        </div>
        <div id="add-new-subject" class="4u$ 12u$(medium) box">
            <a href="#" class="button special big">+ Add more classes</a>
        </div>
        </div>
    </div>
</section>

