<%-include header %>

<!-- Profile Banner -->
<section id="profile-id" class="wrapper style2">
    <div id="bio">
        <a href="#" class="image"><img src="../public/images/pic06.jpg" width="100px" alt="user-photo" /></a>
    </div>
    <div id="name">
        <h2><%- user.name %></h2>
    </div>

    <div id="edit">
        <ul class="actions">
            <li><a href="#" class="button small">Edit profile</a></li>
        </ul>
    </div>

</section>

<!-- Subject section -->
