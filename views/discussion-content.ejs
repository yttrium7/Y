
<% topics.forEach(function(topic){%>
<div class="row box">
    <div class="2u 12u(medium)">
        <a href="/user?username=<%= topic.author%>"><img src="" alt="" width="60px">
            <%= topic.author%>
        </a>
    </div>

    <div class="6u 12u(medium)">
        <h4><a href="/topics/topic?topicId=<%= topic._id%>">
                <%= topic.topicName%>
            </a>
        </h4>
    </div>

    <div class="2u 12u(medium)">
        <h4><%= topic.postTime %></h4>
    </div>

    <div>
        <i class="icon fa-comment"></i>
        <%= topic.viewer %>
    </div>
</div>
<hr>
<%})%>